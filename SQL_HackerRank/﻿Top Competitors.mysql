SELECT A.HACKER_ID, D.NAME
FROM SUBMISSIONS A INNER JOIN CHALLENGES B ON A.CHALLENGE_ID = B.CHALLENGE_ID
                   INNER JOIN DIFFICULTY C ON C.DIFFICULTY_LEVEL = B.DIFFICULTY_LEVEL                                        INNER JOIN HACKERS D ON A.HACKER_ID = D.HACKER_ID
WHERE A.SCORE = C.SCORE
GROUP BY A.HACKER_ID, D.NAME
HAVING COUNT(A.CHALLENGE_ID)>1 
ORDER BY COUNT(A.CHALLENGE_ID) DESC, A.HACKER_ID
